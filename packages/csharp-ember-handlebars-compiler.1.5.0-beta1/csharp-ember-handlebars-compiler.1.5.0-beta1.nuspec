<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>csharp-ember-handlebars-compiler</id>
    <version>1.5.0-beta1</version>
    <title>Precompiler for Ember Handlebars</title>
    <authors>PremyslK</authors>
    <owners>PremyslK</owners>
    <projectUrl>https://github.com/Myslik/csharp-ember-handlebars</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
