      <group targetFramework=".NETFramework4.0">
        <dependency id="Microsoft.AspNet.Web.Optimization" version="1.1.0-Beta1" />
        <dependency id="Microsoft.Web.Infrastructure" version="1.0.0.0" />
        <dependency id="WebGrease" version="1.3.0" />
      </group>
    </dependencies>
  </metadata>
